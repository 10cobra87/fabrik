<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<form>
	<fields name="params">
		<fieldset name="plg-form-subscriptions">
			<field name="subscriptions_accountemail"				repeat="true"				type="text"				description="PLG_FORM_SUBSCRIPTIONS_EMAILACCOUNT_DESC"				label="PLG_FORM_SUBSCRIPTIONS_EMAILACCOUNT_LABEL" />
			<field name="subscriptions_return_msg"				type="textarea"				repeat="true"				rows="5" 				cols="30"				description="PLG_FORM_SUBSCRIPTIONS_RETURN_MSG_DESC"				label="PLG_FORM_SUBSCRIPTIONS_RETURN_MSG_LABEL" />
			<field name="subscriptions_conditon"				type="textarea"				cols="30"				repeat="true"				rows="5"				description="PLG_FORM_SUBSCRIPTIONS_CONDITION_DESC" 				label="PLG_FORM_SUBSCRIPTIONS_CONDITION_LABEL" />							<field 				name="subscriptions_testmode"				type="radio"				repeat="true" 				default="0"				description="PLG_FORM_SUBSCRIPTIONS_TEST_MODE_DESC"				label="PLG_FORM_SUBSCRIPTIONS_TEST_MODE_LABEL" >					<option value="0">JNO</option>					<option value="1">JYES</option>			</field>
			<field 				 name="subscriptions_receive_debug_emails"				 type="radio"				 repeat="true"				 default="1"				 description="PLG_FORM_SUBSCRIPTIONS_DEBUG_EMAIL_DESC"				 label="PLG_FORM_SUBSCRIPTIONS_DEBUG_EMAIL_LABEL">					<option value="0">JNO</option>					<option value="1">JYES</option>			</field>			<field name="subscriptions_send_default_email"				type="radio"				repeat="true"				default="1" description="PLG_FORM_SUBSCRIPTIONS_DEFAULT_EMAIL_DESC"				label="PLG_FORM_SUBSCRIPTIONS_DEFAULT_EMAIL_LABEL">					<option value="0">JNO</option>					<option value="1">JYES</option>			</field>						<field name="subscriptions_test_site"				repeat="true"				type="text"				description="PLG_FORM_SUBSCRIPTIONS_TEST_SITE_DESC"				label="PLG_FORM_SUBSCRIPTIONS_TEST_SITE_LABEL" />							<field				name="subscriptions_test_site_qs"				repeat="true"				type="text"				description="PLG_FORM_SUBSCRIPTIONS_TEST_SITE_QS_DESC"				label="PLG_FORM_SUBSCRIPTIONS_TEST_SITE_QS_LABEL" />		</fieldset>	</fields></form>